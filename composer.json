{
    "name": "elephox/builder-doctrine",
    "description": "Elephox Web Application Builder extension for Doctrine ORM",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 <8.3",
        "doctrine/orm": "^2.12",
        "elephox/web": "^0.4|^0.5",
        "psr/cache-implementation": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "elephox/cache": "dev-develop",
        "elephox/apcu-cache": "dev-develop",
        "symfony/cache": "6.1.x-dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Elephox\\Builder\\Doctrine\\": "src/"
        }
    }
}
